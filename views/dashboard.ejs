<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">	
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Software Licensing as a service</title>
	<link rel="stylesheet" href="../../../../styles/main.css">
	<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Lato:400,700">
	<link rel="shortcut icon" href="../../../../styles/favicon.ico" type="image/x-icon">
</head>
<body class="page-plugins page-secondary" screen_capture_injected="true">
	<div class="navbar navbar-inverse">
		<div class="navbar-inner">
			<div class="container">
				<a href="#" class="brand">
					<span class="logo"></span>
				</a>
				<div class="nav-collapse">
					<ul class="nav">
				        <li class="dropdown">
					        <a href="#" class="icon-power-cord dropdown-toggle" data-toggle="dropdown">Apps <b class="caret"></b></a>
					        <ul class="dropdown-menu">
<% apps.forEach(function(app) { %>
					            <li><a href="http://cpplicense.herokuapp.com/apps/<%=app.id%>"><%=app.description%></a></li>
<% }) %>
					            <li class="divider" style="margin-top:8px"></li>
					            <li><a href="#">New App</a></li>
					        </ul>
					    </li>
					    <li><a href="../../../../logout"><i class="icon-cog"></i><span>Logout</span></a></li>
					</ul>
				</div><!--.nav-collapse-->
			</div>
		</div>
	</div>
	<div class="content">
		<div class="container">
			<div class="row-fluid">
				<div class="span12 page">
					<div class="hero-unit">
						<div class="plugin-unit">
							<div class="span6">
								<h1><%=appinfo.description%></h1>
								<p>Created on <%=appinfo.datecreated%></p>
								<p><small><i>Use the below table to check your customers' use of your software.</br>Create new licenses by adding customers' emails.</i></small>
								</p>
							</div>
							<div class="span6" style="margin-top:25px">
								<pre style="height:128px"><code class="lang-js"><span class="comment">// Your application ID<br/></span>#define APPID <span class="string">"<%= appinfo.client_secret %>"</span><br/><br/><span class="comment">// Your application secret<br/></span>#define APPSECRET <span class="string">"<%= appinfo.client_secret %>"</span></code>
								</pre>
							</div>
						</div>
						<section class="main">
							<div class="span12 dataTables_info" id="plugins-all_info" role="alert" aria-live="polite" aria-relevant="all"><p>Showing <%= licenses.length %> issued licenses</p></div>
							<table id="plugins-all" cellspacing="0" width="100%" class="plugin-list display dataTable no-footer" aria-describedby="plugins-all_info">
								<thead>
									<tr role="row">
										<th class="colPlugin sorting" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label="Plugin: activate to sort column ascending">User email<span class="legend"></span></th>
										<th class="colUpdated sorting_desc_disabled" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label="Updated: activate to sort column descending">From<span class="legend"></span></th>
										<th class="colUpdated sorting_desc_disabled" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label="Updated: activate to sort column descending">To<span class="legend"></span></th>
										<th class="colUpdated sorting_desc_disabled" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label="Updated: activate to sort column descending">Created<span class="legend"></span></th>
										<th class="colDown v sorting_disabled" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label=" Version: activate to sort column descending">Offline time<span class="legend">maximum allowed</span></th>
										<th class="colDown dl sorting_desc" tabindex="0" aria-controls="plugins-all" rowspan="1" colspan="1" aria-label="Downloadslast 30 days: activate to sort column descending" aria-sort="descending">Revoke<span class="legend"></span></th>
									</tr>
								</thead>
								<tbody>
								<tr class="odd">
									<form role="form" action="../../apps/<%=appinfo.id%>/licenses" method="post">
										<td class="">
											<span class="name-description" style="text-align: left;display: block;padding: 9px 0px 0px 10px;color: #6f5a40;font-size: 16px;width: 300px;">
												<span class="title">
													<input name="email" type="search" class="" aria-controls="plugins-all" style="width: 100%;">
												</span>
											</span>
										</td>
										<td class=" ">
											<span style="text-align: left;display: block;padding: 9px 0px 0px 20px;color: #6f5a40;font-size: 16px;width: 90px;">
												<input type="text" id="dpd1" name="from" class="" aria-controls="plugins-all" style="width: 100%;height: 30px;">
											</span>
										</td>
										<td class=" ">
											<span style="text-align: left;display: block;padding: 9px 0px 0px 20px;color: #6f5a40;font-size: 16px;width: 90px;">
												<input type="text" id="dpd2" name="to" class="" aria-controls="plugins-all" style="width: 100%;height: 30px;">
											</span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z">today</span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z">720 hours</span>
												<input type="hidden" name="maxofflinetime" value="720" class="" aria-controls="plugins-all">
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z"><button type="submit" class="btn btn-success">+</button></span>
										</td>
										</form>
									</tr>
<% licenses.forEach(function(license) { %>
								<tr class="odd">
										<td class="">
											<a class="plugin contrib"  data-toggle="collapse" href=".collapse<%=license.id%>">
												<span class="name-description">
													<span class="title"><%=license.email%></span>
												</span>
											</a>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z"><%=license.from%> </span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z"><%=license.to%></span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z"><%=license.datecreated%></span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z"><%=license.maxofflinetime%> hours</span>
										</td>
										<td class=" ">
											<span title="2014-04-02T13:12:09.529Z">
												<a href="../../apps/<%=appinfo.id%>/licenses/<%=license.id%>/delete"><button type="submit" class="btn btn-danger">x</button></a>
											</span>
										</td>
									</tr>
									<tr class="collapse<%=license.id%> collapse">
										<td></td>
							            <td colspan="2">2014, Mar 3rd 04:35:12</td>
							            <td>34.87.9.123</td>
							            <td colspan="2">356NFS2558092</td>
							        </tr>
									<tr class="collapse<%=license.id%> collapse">
										<td></td>
							            <td colspan="2">2014, Mar 3rd 04:35:12</td>
							            <td>34.87.9.123</td>
							            <td colspan="2">356NFS2558092</td>
							        </tr>
									<tr class="collapse<%=license.id%> collapse">
										<td></td>
							            <td colspan="2">2014, Mar 3rd 04:35:12</td>
							            <td>34.87.9.123</td>
							            <td colspan="2">356NFS2558092</td>
							        </tr>
									<tr class="collapse<%=license.id%> collapse">
										<td></td>
							            <td colspan="2">2014, Mar 3rd 04:35:12</td>
							            <td>34.87.9.123</td>
							            <td colspan="2">356NFS2558092</td>
							        </tr>
<% }) %>
								</tbody>
							</table>
						</section>
					</div>
				</div>
			</div>
		</div>
	</div>
	<footer class="grunt-footer">
		<div class="inner">
			<div class="container">
				<ul class="pull-right">
					<li class="social"><a href="https://github.com/li0nel/cpplicense" alt="CppLicense on GitHub"><span class="icon-github"></span></a></li>
